# Depends on:  

name=mc
version=4.8.24
release=1
source=(https://github.com/MidnightCommander/mc/archive/$version.tar.gz)

build() {
	cd $name-$version
	./configure --prefix=/usr
        make
        make DESTDIR=$PKG install
}
